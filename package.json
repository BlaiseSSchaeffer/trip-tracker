{
  "name": "trip-tracker",
  "version": "1.0.0",
  "description": "Trip tracking and planning at its best!",
  "main": "src/index.ts",
  "scripts": {
    "build": "npm run buildSchema && tsc",
    "buildSchema": "find src -name 'typeDefs.graphql' -exec paste {} > build/schema.graphql \\;",
    "commitlint": "commitlint",
    "dev": "nodemon",
    "prettier": "pretty-quick",
    "start": "node build/index.js",
    "test": "echo \"No test specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/BlaiseSSchaeffer/trip-tracker.git"
  },
  "author": "Blaise Schaeffer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BlaiseSSchaeffer/trip-tracker/issues"
  },
  "homepage": "https://github.com/BlaiseSSchaeffer/trip-tracker#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@types/config": "0.0.34",
    "@types/express": "^4.17.1",
    "@types/recursive-readdir": "^2.2.0",
    "husky": "^3.0.5",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.3",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "recursive-readdir": "^2.2.2"
  }
}
